{
  "eslint.workingDirectories": [
    {
      "directory": "./",
      "pattern": ["!**/Accepted/**"]
    }
  ],
  "C_Cpp.codeAnalysis.exclude": {
    "**/Accepted/**": true
  },
  "C_Cpp.codeAnalysis.clangTidy.headerFilter": ".*/Accepted/.*",
  "files.associations": {
    "xiosbase": "cpp",
    "vector": "cpp",
    "xutility": "cpp",
    "xstring": "cpp",
    "deque": "cpp",
    "initializer_list": "cpp",
    "list": "cpp",
    "ranges": "cpp",
    "span": "cpp",
    "xhash": "cpp",
    "utility": "cpp",
    "unordered_map": "cpp"
  }
}